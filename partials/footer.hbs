{{!
This footer template is shared across all the pages.
}}
<footer class="m-footer">
  <div class="m-footer__content">
    <p class="m-footer-copyright">
      <span>
        {{@site.title}}
        ©
        {{date format="YYYY"}}
      </span>
      <span>
        •
      </span>
      <a href="https://psgtech.edu" target="_blank" rel="noopener">
        <span>
          {{t "PSG College of Technology"}}
        </span>
      </a>
    </p>
    
    {{#if @site.secondary_navigation}}
      <nav class="m-footer__nav-secondary" role="navigation" aria-label="{{t "Secondary menu in footer"}}">
        {{navigation type="secondary"}}
      </nav>
    {{/if}}
    <nav class="m-footer-social">
      {{#if @site.facebook}}
        <a
          href="{{facebook_url @site.facebook}}"
          target="_blank"
          rel="noopener"
          aria-label="Facebook"
        >
          <span class="icon-facebook"></span>
        </a>
      {{/if}}
      {{#if @site.twitter}}
        <a
          href="{{twitter_url @site.twitter}}"
          target="_blank"
          rel="noopener"
          aria-label="Twitter"
        >
          <span class="icon-twitter"></span>
        </a>
      {{/if}}
      <a
        href="https://www.instagram.com/thebridgepsg"
        target="_blank"
        rel="noopener"
        aria-label="Instagram"
      >
        <span class="icon-instagram"></span>
      </a>
      <a
        href="https://www.linkedin.com/company/the-bridge-psg-tech"
        target="_blank"
        rel="noopener"
        aria-label="LinkedIn"
      >
        <span class="icon-linkedin"></span>
      </a>
      <a
        href="{{@site.url}}/rss"
        target="_blank"
        rel="noopener"
        aria-label="rss"
      >
        <span class="icon-rss"></span>
      </a>
    </nav>
